# Environment Configuration

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL_TRANSCRIPTION=whisper-1
OPENAI_MODEL_ORCHESTRATION=gpt-4.1
OPENAI_MODEL_ANALYSIS=gpt-4.1

# Application Configuration
NODE_ENV=development
PORT=3000
LOG_LEVEL=info

# Audio Processing Configuration
AUDIO_SAMPLE_RATE=16000
AUDIO_CHANNELS=1
AUDIO_CHUNK_SIZE=1024

# Real-time Processing Configuration
WEBSOCKET_PORT=8080
PROCESSING_INTERVAL=100
SENTIMENT_THRESHOLD=0.7
ESCALATION_THRESHOLD=0.8

# Agent Assist Configuration
CHATBOT_ENABLED=true
INSIGHTS_ENABLED=true
CALL_REASON_TRACKING=true
ESCALATION_DETECTION=true

# Database Configuration (if needed)
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME=agent_assist
# DB_USER=postgres
# DB_PASSWORD=password

# AWS Configuration (for deployment)
# AWS_REGION=us-east-1
# AWS_ACCESS_KEY_ID=your_access_key
# AWS_SECRET_ACCESS_KEY=your_secret_key
